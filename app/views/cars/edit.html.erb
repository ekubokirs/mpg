<%= form_for @car, url: car_path(@car) do |f| %>
	<%= f.label :make %>
	<%= f.text_field :make %>
	<%= f.label :brand, "Model" %>
	<%= f.text_field :brand %>
	<%= f.label :year %>
	<%= f.text_field :year %>
	<%= f.label :color %>
	<%= f.text_field :color %>
	<%= f.label :current_miles, "Current Milage" %>
	<%= f.text_field :current_miles %>
	<%= f.submit "Submit" %>
<% end %>